{
  "name": "nanify",
  "description": "Un kit de componentes para React basado en TypeScript.",
  "author": "Genaro Gonzalez",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "version": "0.2.48",
  "type": "module",
  "files": [
    "src/components",
    "src/sass",
    "src/functions",
    "src/app/api",
    "env.ts",
    "fn"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "alias": "node ./fn/aggAlias.js",
    "upVersion": "node ./fn/upVersion.js",
    "update:jsx": "node ./fn/updateTsconfig.js",
    "publicar": "npm run upVersion && npm run update:jsx && npm publish"
  },
  "dependencies": {
    "password-validator": "^5.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
