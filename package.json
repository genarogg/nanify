{
  "name": "nanify",
  "description": "Un kit de componentes para React basado en TypeScript.",
  "author": "Genaro Gonzalez",
  "license": "MIT",
  "version": "0.1.94",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/styles",
    "postinstall.js",
    "movefile.js"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build-lib": "tsup src/components --format cjs,esm --dts",
    "publicar": "npm run build-lib && npm publish",
    "postinstall": "node ./postinstall.js"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "next": "14.2.18",
    "react-toastify": "^10.0.6"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.18",
    "tsup": "^6.7.0",
    "typescript": "^5"
  }
}
