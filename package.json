{
  "name": "nanify",
  "description": "Un kit de componentes para React basado en TypeScript.",
  "author": "Genaro Gonzalez",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "version": "0.2.51",
  "files": [
    "src/components",
    "src/sass",
    "src/functions",
    "src/app/api",
    "env.ts",
    "fn"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "start": "vite preview",
    "alias": "node ./fn/aggAlias.js",
    "upVersion": "node ./fn/upVersion.js",
    "update:jsx": "node ./fn/updateTsconfig.js",
    "publicar": "npm run upVersion && npm run update:jsx && npm publish",
    "postinstall": "node ./fn/postinstall.js",
    "nanify": "node ./node_modules/nanify/fn/moveComponents.js",
    "@nano": "node ./node_modules/nanify/fn/moveComponents.js",
    "server": "json-server --port 3001 --watch server/db.json",
    "generate": "node ./server/table-fake-data.js"
  },
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "@justinribeiro/lite-youtube": "^1.7.1",
    "@tippyjs/react": "^4.2.6",
    "algoliasearch": "^5.23.3",
    "crypto-js": "^4.2.0",
    "graphql": "^16.10.0",
    "immer": "^10.1.1",
    "json-server": "^1.0.0-beta.3",
    "lucide-react": "^0.488.0",
    "motion": "^12.7.2",
    "npm-run-all": "^4.1.5",
    "ogl": "^1.0.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-google-recaptcha-v4": "^2.0.1",
    "react-icons": "^5.5.0",
    "react-instantsearch": "^7.15.5",
    "react-instantsearch-nextjs": "^0.4.6",
    "react-lazy-load-image-component": "^1.6.3",
    "react-loading-skeleton": "^3.5.0",
    "react-router-dom": "^7.5.0",
    "react-toastify": "^11.0.5",
    "react-youtube": "^10.1.0",
    "sass": "^1.86.3",
    "simplex-noise": "^4.0.3",
    "sonner": "^2.0.7",
    "swiper": "^11.2.6",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/crypto-js": "^4.2.2",
    "@types/css-modules": "^1.0.5",
    "@types/graphql": "^14.2.3",
    "@types/node": "^22.14.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-lazy-load-image-component": "^1.6.4",
    "@types/swiper": "^5.4.3",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
