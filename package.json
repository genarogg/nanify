{
  "name": "nanify",
  "description": "Un kit de componentes para React basado en TypeScript.",
  "author": "Genaro Gonzalez",
  "license": "MIT",
  "version": "0.2.11",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/styles",
    "src/functions",
    "src/app/api",
    "postinstall.js",
    "movefile.js"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "movefile": "node movefile.js",
    "update:jsx": "node updateTsconfig.js",
    "clean-dist": "npm run update:jsx &&  rimraf dist",
    "build-lib": "npm run clean-dist && tsup src/components --format cjs,esm --dts",
    "publicar": "npm run build-lib && npm publish",
    
    "algolia-index-demo": "node src/components/algolia/indexDemo.js"
  },
  "dependencies": {
    "@plaiceholder/next": "^3.0.0",
    "algoliasearch": "^5.18.0",
    "crypto-js": "^4.2.0",
    "nanify": "^0.2.11",
    "next": "15.1.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-icons": "^5.4.0",
    "react-loading-skeleton": "^3.5.0",
    "react-toastify": "^11.0.2",
    "sass": "^1.83.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.2",
    "json": "^11.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5"
  }
}