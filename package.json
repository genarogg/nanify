{
  "name": "nanify",
  "description": "Un kit de componentes para React basado en TypeScript.",
  "author": "Genaro Gonzalez",
  "license": "MIT",
  "version": "0.2.7",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/styles",
    "src/functions",
    "postinstall.js",
    "movefile.js"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "update:jsx": "node updateTsconfig.js",
    "clean-dist": "npm run update:jsx &&  rimraf dist",
    "build-lib": "npm run clean-dist && tsup src/components --format cjs,esm --dts",
    "publicar": "npm run build-lib && npm publish"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "nanify": "^0.2.7",
    "next": "14.2.18",
    "react-google-recaptcha-v3": "^1.10.1",
    "react-icons": "^5.4.0",
    "react-toastify": "^10.0.6"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.18",
    "json": "^11.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^6.7.0",
    "typescript": "^5"
  }
}
