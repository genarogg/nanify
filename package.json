{
  "name": "nanify",
  "description": "Un kit de componentes para React basado en TypeScript.",
  "author": "Genaro Gonzalez",
  "license": "MIT",
  "version": "0.1.28",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/sass",
    "src/sass_backup",
    "postinstall.js"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build-css": "postcss ./src/sass/**/*.scss --dir dist",
    "build-ts": "tsup",
    "publicar": "npm run build-css && npm run build-ts && npm publish",
    "move": "node ./moveFile.js",
    "postinstall": "node ./postinstall.js"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.3.0",
    "react-toastify": "^10.0.6",
    "sass": "^1.81.0"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "next": "14.2.18",
    "react-router-dom": "^7.0.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.2.18",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-modules": "^6.0.1",
    "postcss-scss": "^4.0.9",
    "ts-node": "^10.9.2",
    "tsup": "^6.7.0",
    "typescript": "^5.7.2"
  }
}
