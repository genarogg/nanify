{
  "name": "nanify",
  "description": "Un kit de componentes para React basado en TypeScript.",
  "author": "Genaro Gonzalez",
  "license": "MIT",
  "version": "0.2.40",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/components",
    "src/sass",
    "src/functions",
    "src/app/api",
    "env.ts",
    "fn"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "update:jsx": "node ./fn/updateTsconfig.js",
    "clean-dist": "npm run update:jsx &&  rimraf dist",
    "build-lib": "npm run clean-dist && tsup",
    "publicar": "npm run build-lib && npm publish",
    "postinstall": "node ./fn/postinstall.js",
    "nanify": "node ./node_modules/nanify/fn/moveComponents.js"
  },
  "dependencies": {
    "@plaiceholder/next": "^3.0.0",
    "@tippyjs/react": "^4.2.6",
    "algoliasearch": "^5.18.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "motion": "^11.15.0",
    "nanify": "^0.2.36",
    "next": "14.2.22",
    "react": "^18",
    "react-dom": "^18",
    "react-google-recaptcha-v3": "^1.10.1",
    "react-icons": "^5.4.0",
    "react-instantsearch": "^7.13.9",
    "react-instantsearch-nextjs": "^0.3.20",
    "react-loading-skeleton": "^3.5.0",
    "react-toastify": "^11.0.2",
    "sass": "^1.83.0",
    "sharp": "^0.33.5",
    "swiper": "^11.2.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.22",
    "json": "^11.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5"
  }
}
