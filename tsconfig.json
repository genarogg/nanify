{
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@fn": [
        "./src/functions/index.ts"
      ],
      "@env": [
        "./env.ts"
      ],
      "@public/*": [
        "./public/*"
      ],
      "@nano": [
        "./src/components/nano/index.tsx"
      ],
      "@btn/*": [
        "./src/components/btns/*"
      ],
      "@slider/*": [
        "./src/components/slider/*"
      ],
      "@hook/*": [
        "./src/hook/*"
      ],
      "@tooltip": [
        "./src/components/tooltip/index.tsx"
      ],
      "@sections/*": [
        "./src/components/sections/*"
      ],
      "@ux": [
        "./src/components/ux/index.tsx"
      ],
      "@context/*": [
        "./src/context/*"
      ],
    },
    "jsx": "react-jsx",
    "target": "ES2022",
    "useDefineForClassFields": true,
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable",
      "ES2023"
    ],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "isolatedModules": true,
    "moduleDetection": "force",
    "noEmit": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.combined.tsbuildinfo"
  },
  "include": [
    "src",
    "vite.config.ts",
    "src/types/declarations.d.ts"
  ]
}