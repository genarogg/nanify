{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "incremental": true,
    "tsBuildInfoFile": "./dist/tsconfig.tsbuildinfo",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@nanify": ["./src/lib/index.tsx"],
      "@fn/*": ["./src/functions/*"],
      "@env": ["./env.ts"]
    },
    "outDir": "./dist",
    "declaration": true,
    "declarationDir": "./dist/types",
    "target": "ES2017",
    "plugins": [ {"name": "next"} ]
  },
  "include": [
    "**/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "src/components/algolia/indexDemo.js",
    "env.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}